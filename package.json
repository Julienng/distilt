{
  "name": "distilt",
  "version": "0.12.1-0",
  "description": "generate publishable bundles",
  "keywords": [
    "esbuild",
    "bundle"
  ],
  "homepage": "https://github.com/sastan/distilt#readme",
  "bugs": "https://github.com/sastan/distilt/issues",
  "repository": "github:sastan/distilt",
  "license": "MIT",
  "author": "Sascha Tandel (https://github.com/sastan)",
  "type": "module",
  "bin": "./distilt.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "format": "prettier --write --ignore-path .gitignore . '!pnpm-lock.yaml'",
    "release": "npx np --yolo",
    "preinstall": "npx only-allow pnpm"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "dependencies": {
    "es-module-lexer": "^0.9.3",
    "esbuild": "^0.14.11",
    "execa": "^6.0.0",
    "find-up": "^6.2.0",
    "globby": "^12.0.2",
    "merge-source-map": "^1.1.0",
    "normalize-package-data": "^3.0.3",
    "rollup": "^2.63.0",
    "rollup-plugin-dts": "^4.1.0"
  },
  "devDependencies": {
    "only-allow": "^1.0.0",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.5",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/"
  },
  "engines": {
    "node": ">=14.6"
  }
}
