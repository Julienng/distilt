{
  "name": "distilt",
  "version": "0.10.1",
  "description": "generate publishable bundles",
  "keywords": [
    "esbuild",
    "bundle"
  ],
  "homepage": "https://github.com/sastan/distilt#readme",
  "bugs": "https://github.com/sastan/distilt/issues",
  "repository": "github:sastan/distilt",
  "license": "MIT",
  "author": "Sascha Tandel (https://github.com/sastan)",
  "main": "./distilt.js",
  "bin": "./distilt.js",
  "files": [
    "distilt.js"
  ],
  "scripts": {
    "format": "prettier --write --ignore-path .gitignore .",
    "release": "npx np"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "dependencies": {
    "es-module-lexer": "^0.3.26",
    "esbuild": "^0.9.3",
    "execa": "^5.0.0",
    "find-up": "^5.0.0",
    "globby": "^11.0.1",
    "pkg-dir": "^5.0.0",
    "project-root-directory": "^1.0.3",
    "rollup": "^2.35.1",
    "rollup-plugin-dts": "^2.0.1"
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "size-limit": "^4.9.1",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/"
  },
  "engines": {
    "node": ">=10.13"
  }
}
